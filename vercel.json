{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "dhmguide.com"
        }
      ],
      "destination": "https://www.dhmguide.com/:path*",
      "permanent": true
    },
    {
      "source": "/blog/:slug*",
      "destination": "/never-hungover/:slug*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/never-hungover/:slug",
      "headers": [
        {
          "key": "Link",
          "value": "<https://www.dhmguide.com/never-hungover/:slug>; rel=\"canonical\""
        }
      ]
    }
  ],
  "trailingSlash": false
}